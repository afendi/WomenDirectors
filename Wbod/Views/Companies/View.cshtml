@model IEnumerable<Wbod.Models.DB.Companies>

@{
    ViewData["Title"] = "View";
}

<h2>View</h2>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
                <th>
                    @Html.DisplayNameFor(model => model.Companyname)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Companyboardsize)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Companyisactive)
                </th>
            <th>
                @Html.DisplayNameFor(model => model.CompanylistNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CompanysectorNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CompanytypeNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IsglcNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Companyname)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Companyboardsize)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Companyisactive)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CompanylistNavigation.Companylistsname)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CompanysectorNavigation.Sectornames)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CompanytypeNavigation.Typenames)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IsglcNavigation.Isglcstatus)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
