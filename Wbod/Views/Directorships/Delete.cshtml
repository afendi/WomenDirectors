@model Wbod.Models.DB.Directorship

@{
    ViewData["Title"] = "Delete";
}

<div class="bd-example">
    <h4>Are you sure you want to delete @Html.DisplayFor(model => model.Director.Name) from @Html.DisplayFor(model => model.Company.Companyname) for year  @Html.DisplayFor(model => model.Session.Sessionyear)?</h4>
</div>


<div>    
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="Id" />
        <input type="hidden" id="Companyid" name="Companyid" value="@Model.Companyid" />
        <a a href="javascript:void(0);" onclick="history.go(-1);" class="btn btn-secondary">Cancel</a> |
        <input type="submit" value="Delete" class="btn btn-danger" />
    </form>
</div>
